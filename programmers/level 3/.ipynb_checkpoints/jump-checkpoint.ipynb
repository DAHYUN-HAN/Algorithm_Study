    "def solution(n):\n",
    "    if(n == 1):\n",
    "        return 1\n",
    "    elif(n == 2):\n",
    "        return 2\n",
    "    d = [0]* 2000\n",
    "    \n",
    "    d[1] = 1\n",
    "    d[2] = 2\n",
    "    \n",
    "    for i in range(3, n+1):\n",
    "        d[i] = d[i-2] + d[i-1]\n",
    "        \n",
    "    return d[n] % 1234567"
   ]
